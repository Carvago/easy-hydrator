{
    "name": "eag/easy-hydrator",
    "description": "Hydrate arrays to objects easily with PHP 8.0 and constructor injection",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2|^4.0",
        "phpstan/phpdoc-parser": "^1.2|^2.1",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/string": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "EAG\\EasyHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EAG\\EasyHydrator\\Tests\\": "tests"
        }
    }
}

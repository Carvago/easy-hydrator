{
    "name": "symplify/easy-hydrator",
    "description": "Hydrate arrays to objects easily with PHP 7.4 and constructor injection",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "phpstan/phpdoc-parser": "^0.5|^1.2",
        "symfony/config": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "symfony/framework-bundle": "^5.3|^6.0",
        "symfony/http-kernel": "^5.3|^6.0",
        "symfony/string": "^5.3|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyHydrator\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
